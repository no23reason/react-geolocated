{
  "name": "react-geolocated",
  "description": "React.js Higherâ€“Order Component for using Geolocation API",
  "author": "Dan Homola",
  "user": "no23reason",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest --coverage",
    "test:tdd": "jest --watch",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --ignore-pattern docs --cache",
    "test:ts": "cd tests/typescript && typings install && tsc && rimraf out && rimraf typings",
    "make-docs": "webpack",
    "dist": "webpack",
    "dist:min": "webpack",
    "dist:modules": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "pretest": "rimraf tests/typescript/out && rimraf tests/typescript/typings && npm run test:lint && npm run test:ts",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run make-docs",
    "postinstall": "node lib/post_install.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "dist-modules",
  "typings": "./index.d.ts",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "~6.18.0",
    "babel-plugin-transform-object-rest-spread": "~6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.9.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.25.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "git-prepush-hook": "^1.0.1",
    "highlight.js": "^9.4.0",
    "html-webpack-plugin": "^2.21.0",
    "html-webpack-remark-plugin": "^0.1.0",
    "jest": "^16.0.0",
    "json-loader": "^0.5.4",
    "purecss": "^0.6.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-ghfork": "^0.3.3",
    "react-test-renderer": "^15.3.2",
    "rimraf": "^2.5.3",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.1",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "typescript": "~2.0.3",
    "typings": "^1.3.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.15.0"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/no23reason/react-geolocated.git"
  },
  "homepage": "https://no23reason.github.io/react-geolocated/",
  "bugs": {
    "url": "https://github.com/no23reason/react-geolocated/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "geolocation"
  ],
  "license": "MIT",
  "pre-push": [
    "test",
    "test:lint"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
